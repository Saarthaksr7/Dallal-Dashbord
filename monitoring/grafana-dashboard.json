                        "mode": "absolute"
                    }
                }
            },
            "title": "HTTP Request Rate",
            "targets": [
                {
                    "expr": "rate(http_requests_total[5m])",
                    "legendFormat": "{{method}} {{path}}"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "id": 1,
            "type": "timeseries"
        },
        {
            "datasource": "Prometheus",
            "fieldConfig": {
                "defaults": {
                    "unit": "ms"
                }
            },
            "title": "Response Time (p95)",
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, http_request_duration_seconds_bucket)",
                    "legendFormat": "p95"
                },
                {
                    "expr": "histogram_quantile(0.50, http_request_duration_seconds_bucket)",
                    "legendFormat": "p50"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "id": 2,
            "type": "timeseries"
        },
        {
            "datasource": "Prometheus",
            "title": "Email Digest Buffer Size",
            "targets": [
                {
                    "expr": "digest_buffer_size",
                    "legendFormat": "Buffer Count"
                }
            ],
            "gridPos": {
                "h": 6,
                "w": 8,
                "x": 0,
                "y": 8
            },
            "id": 3,
            "type": "stat"
        },
        {
            "datasource": "Prometheus",
            "title": "Active Connections",
